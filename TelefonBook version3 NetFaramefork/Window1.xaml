<Window x:Class="TelefonBook_version3_NetFaramefork.Window1"
         xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="447" Width="256" Loaded="Window_Loaded">
    <Window.Background>
        <ImageBrush/>
    </Window.Background>
    <Grid Background="#FF8D8D8D" MinHeight="430" MaxWidth="261" MaxHeight="261" Margin="0,-28,0,-20">
        <TextBox x:Name="PhoneNumber" Height="35" Margin="0,77,0,0" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" TextChanged="PhoneNumber_TextChanged" Background="{x:Null}" FontSize="18" BorderBrush="#FF8D8D8D"/>
        <DataGrid HorizontalAlignment="Left" Height="172" Margin="10,8,0,0" VerticalAlignment="Top" Width="228" SelectionChanged="DataGrid_SelectionChanged"/>

    </Grid>
</Window>